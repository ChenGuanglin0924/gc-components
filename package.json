{
  "name": "gc-components",
  "version": "0.0.3",
  "description": "React + Hooks + TypeScript Components",
  "private": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:css && npm run check:types",
    "build:cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel components -d lib --source-maps --ignore **/__tests__",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel components -d es --source-maps --ignore **/__tests__",
    "build:umd": "webpack --config ./build/webpack.config.prod.js",
    "build:css": "gulp --gulpfile ./build/gulpfile.js compile",
    "build:sb": "rimraf docs && build-storybook -o docs",
    "check:types": "tsc",
    "dev:sb": "start-storybook -p 6006",
    "prepublish": "npm run build"
  },
  "files": [
    "lib",
    "es",
    "dist",
    "types"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChenGuanglin0924/gc-components.git"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Hooks",
    "Components"
  ],
  "author": "garaschan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChenGuanglin0924/gc-components/issues"
  },
  "homepage": "https://github.com/ChenGuanglin0924/gc-components#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "core-js": "^3.3.4",
    "rc-tooltip": "^4.0.0-alpha.3",
    "react": "^16.13.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
