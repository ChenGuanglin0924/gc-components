{
  "name": "gc-components",
  "version": "0.1.0",
  "description": "React + Hooks + TypeScript Components",
  "private": false,
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:css",
    "build:cjs": "rimraf lib && tsc -b ./build/tsconfig.cjs.json",
    "build:es": "rimraf es && tsc -b ./build/tsconfig.es.json",
    "build:umd": "webpack --config ./build/webpack.config.umd.js",
    "build:css": "gulp --gulpfile ./build/gulpfile.js compile",
    "build:sb": "rimraf docs && build-storybook -o docs",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --ext .tsx,.ts --fix components"
  },
  "pre-commit": [
    "eslint"
  ],
  "files": [
    "lib",
    "es",
    "dist",
    "types"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChenGuanglin0924/gc-components.git"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Hooks",
    "Components"
  ],
  "author": "garaschan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChenGuanglin0924/gc-components/issues"
  },
  "homepage": "https://github.com/ChenGuanglin0924/gc-components#readme",
  "sideEffects": false,
