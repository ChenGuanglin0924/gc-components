{
  "name": "gc-react-components",
  "version": "0.0.1",
  "description": "React+ReactHooks Components",
  "private": false,
  "scripts": {
    "build:publish": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:css",
    "build:umd": "webpack --config ./build/webpack.config.prod.js",
    "build:cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel components -d lib --source-maps --ignore **/__tests__",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel components -d es --source-maps --ignore **/__tests__",
    "build:css": "gulp --gulpfile ./build/gulpfile.js compile"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChenGuanglin0924/gc-react-components.git"
  },
  "keywords": [
    "React",
    "ReactHooks",
    "Components"
  ],
  "author": "GarasChan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChenGuanglin0924/gc-react-components/issues"
  },
  "homepage": "https://github.com/ChenGuanglin0924/gc-react-components#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "core-js": "^3.3.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "autoprefixer": "^9.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-filesize": "0.0.6",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "regenerator-runtime": "^0.13.3",
    "typescript": "^3.6.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "browserslist": [
    "last 2 versions",
